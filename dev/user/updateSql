ALTER TABLE `Agency` ADD `tel1` VARCHAR( 20 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `tel2` VARCHAR( 20 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `tel3` VARCHAR( 20 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `email` VARCHAR( 255 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `address` VARCHAR( 400 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `city` VARCHAR( 255 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `zipCode` VARCHAR( 10 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL ,
ADD `contact` VARCHAR( 300 ) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL 


